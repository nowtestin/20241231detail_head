        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   //Type appropriate comment here, and begin script below
       // MRVSを変数に代入
    var mrvs = g_service_catalog.parent.getValue('sinsei_jouhou');

    // 1行目の場合
    if (!g_form.getValue('v_detail_no')) {
        if (mrvs.length === 0) {
            g_form.setValue('v_detail_no', 1);

        // 1行目ではない場合
        // MRVSのデータをJSONに変換し、現在の行+1を変数「行」に入力する
        } else if (g_service_catalog.parent) {
            var max_row = 0;
            var mrvs_list = JSON.parse(mrvs);

            for (var i = 0; i < mrvs_list.length; i++) {
                var current_row = Number(mrvs_list[i].v_detail_no);

                if (current_row > max_row)
                    max_row = current_row;
            }
            g_form.setValue('v_detail_no', Number(max_row) + 1);
        }
    }
}]]></script>
